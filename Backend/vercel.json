{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",           
        "use": "@vercel/node",          
        "config": {
          "maxDuration": 10             
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",                  
        "dest": "/src/index.js"          
      }
    ]
  }
  